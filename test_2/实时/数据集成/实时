CREATE TABLE `sink_TESTXN_xqs_user`(`id` DECIMAL(38,0),`password` STRING,`username` STRING,PRIMARY KEY (id) NOT ENFORCED ) WITH ('connector' = 'jdbc','datasource.name' = 'xqs_oracle','table-name' = 'TESTXN."xqs_user"');
